
public class BooleanMethods {

	/**
	 * 
	 * @param x
	 * @return
	 */

	public static boolean isSingleDigit(int x) {
		if (x > -10 && x < 10) {
			return true;
		} else {
			return false;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(isSingleDigit(99));

	}

}
