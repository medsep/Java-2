
public class Strings {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String fruit = "banana";
		char letter = fruit.charAt(0);
		System.out.println(letter);
		if (letter == 'b') {
			System.out.println('?');
		}
		
		System.out.print("Roman alphabet: ");
		for (char c = 'A'; c <= 'Z'; c++) {
		System.out.print(c);
		}
		System.out.println();

	}

}
