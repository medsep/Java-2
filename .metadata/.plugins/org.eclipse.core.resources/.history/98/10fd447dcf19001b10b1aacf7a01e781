
public class Student {
	int size;

	int[] counts = new int[4];
	double[] values = new double[size];

	int i = 0;
	{
		while (i < 4) {
			System.out.println(counts[i]);
			i++;
		}
	}

	public static void printArray(int[] a) {
		System.out.print("{" + a[0]);
		for (int i = 1; i < a.length; i++) {
			System.out.print(", " + a[i]);
		}
		System.out.println("}");
	}

}
