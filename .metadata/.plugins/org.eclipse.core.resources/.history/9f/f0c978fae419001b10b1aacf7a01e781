
public class ScannerE {

	public static void main(String[] args) {
		String name;
		int age;
		Scanner in = new Scanner;
		System.out.print("What is your name? ");
		name = in.nextLine();
		System.out.print("What is your age? ");
		age = in.nextInt();
		System.out.printf("Hello %s, age %d\n", name, age);

	}

}
